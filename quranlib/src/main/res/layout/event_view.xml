                    android:gravity="center_vertical|left"
                    android:text="@string/calander_al_hijira"
                    android:textColor="@color/onPrimary1"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/islMonth1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="center_vertical|left"
                    android:text="@string/muharram_1236_ah"
                    android:textColor="@color/onPrimary3"
                    android:textSize="@dimen/size_12" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="center_vertical|right"
            android:orientation="vertical"
            tools:ignore="RtlHardcoded">

            <TextView
                android:id="@+id/engDate1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_margin="8dp"
                android:layout_weight="1"
                android:gravity="right"
                android:text="@string/oct_24_2012"
                android:textColor="@color/onSurface2"
                android:textSize="14sp"
                android:textStyle="bold"
                tools:ignore="NestedWeights" />
        </LinearLayout>
    </LinearLayout>

    <View
